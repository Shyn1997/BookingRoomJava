<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="bookingroom" match-name=".*"/>
  <table-filter match-catalog="bookingroom" match-name="address"/>
  <table-filter match-catalog="bookingroom" match-name="apartment"/>
  <table-filter match-catalog="bookingroom" match-name="category"/>
  <table-filter match-catalog="bookingroom" match-name="city"/>
  <table-filter match-catalog="bookingroom" match-name="district"/>
  <table-filter match-catalog="bookingroom" match-name="posts"/>
  <table-filter match-catalog="bookingroom" match-name="user"/>
</hibernate-reverse-engineering>